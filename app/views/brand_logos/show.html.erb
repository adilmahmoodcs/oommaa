<div class="card">
  <div class="card-block">
    <h3>
      <%= @brand.name %>
    </h3>
    <div class="row">
      <div class="col-sm-12 col-md-2 brand-logo-holder">
        <%= image_tag @logo.try(:image).url(:thumb), width: "100px" %>
      </div>
      <div class="col-sm-12 col-md-10">
        <div class="user-details">
        <div class="col-sm-12 col-md-6 col-lg-3">
          <div class="holder">
            <h5>
              <%= t(:trademark_registration_number)%>
            </h5>
            <h4>
              <%= @logo.try(:trademark_registration_number)%>
            </h4>
          </div>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-3">
          <div class="holder">
            <h5>
              <%= t(:trademark_registration_location)%>
            </h5>
            <h4>
              <%= @logo.try(:trademark_registration_location)%>
            </h4>
          </div>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-3">
          <div class="holder">
            <h5>
              <%= t(:trademark_registration_category)%>
            </h5>
            <h4>
              <%= @logo.try(:trademark_registration_category)%>
            </h4>
          </div>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-3">
          <div class="holder">
            <h5>
              <%= t(:trademark_registration_url)%>
            </h5>
            <h4 class="truncate">
              <%= @logo.try(:trademark_registration_url)%>
            </h4>
          </div>
        </div>
        </div>

      </div>
    </div>
  </div>
</div>
