<div class="row">
  <div class="col-md-offset-2 col-md-10">

    <div class="form-group">
      <%= f.label :visa_id, class: "col-sm-2 control-label" %>
      <div class="col-sm-6">
        <%= f.text_field :visa_id, class: 'form-control', required: true %>
      </div>
    </div>

    <div class="form-group">
      <%= f.label :name, class: "col-sm-2 control-label" %>
      <div class="col-sm-6">
        <%= f.text_field :name, class: 'form-control', required: true %>
      </div>
    </div>

    <div class="form-group">
      <%= f.label :issue, class: "col-sm-2 control-label" %>
      <div class="col-sm-6">
        <%= f.text_field :issue, class: 'form-control datetimepickerYearsMode' %>
      </div>
    </div>


    <div class="form-group">
      <%= f.label :finish, class: "col-sm-2 control-label" %>
      <div class="col-sm-6">
        <%= f.text_field :finish, class: 'form-control datetimepickerYearsMode' %>
      </div>
    </div>

    <div class="form-group">
      <%= f.label :completed, class: "col-sm-2 control-label" %>
      <div class="col-sm-6">
        <%= f.text_field :completed, class: 'form-control datetimepickerYearsMode' %>
      </div>
    </div>

    <div class="form-group">
      <%= f.label :notes, class: "col-sm-2 control-label" %>
      <div class="col-sm-6">
        <%= f.text_area :notes, class: 'form-control', rows: 5 %>
      </div>
    </div>

  </div>
</div>
